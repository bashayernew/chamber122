{
  "version": 2,
  "buildCommand": "echo 'Static site - no build required'",
  "outputDirectory": ".",
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/admin-login",
      "destination": "/admin-login.html"
    },
    {
      "source": "/admin-dashboard",
      "destination": "/admin-dashboard.html"
    },
    {
      "source": "/auth",
      "destination": "/auth.html"
    },
    {
      "source": "/directory",
      "destination": "/directory.html"
    },
    {
      "source": "/events",
      "destination": "/events.html"
    },
    {
      "source": "/inbox",
      "destination": "/inbox.html"
    },
    {
      "source": "/owner",
      "destination": "/owner.html"
    },
    {
      "source": "/owner-form",
      "destination": "/owner-form.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/bulletin",
      "destination": "/bulletin.html"
    }
  ]
}
